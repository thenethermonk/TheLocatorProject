{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES6",
    "moduleResolution": "Node",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./scripts",
    "outDir": "./TheLocatorProject BP/scripts",
    "removeComments": true
  },
  "exclude": ["node_ampules"]
}
